<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.server.dao.UserMapper">

    <update id="updateUser" parameterType="com.demo.server.dto.UserDTO">
        update test_user set user_name=#{user.userName}, address=#{user.address} where user_id = #{user.userId}
    </update>
</mapper>